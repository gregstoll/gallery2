class AdminRepositoryDownloadAndInstallControllerTestUrlGenerator {
	public function generateUrl($params = array(), $options = array()) {
		$buf = array();

		foreach ($params as $key => $value) {
			$buf[] = "$key=$value";
		}

		return implode(',', $buf);
	}
}

